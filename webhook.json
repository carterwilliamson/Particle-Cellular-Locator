{
  "eventName":"deviceLocator",
  "url": "https://www.googleapis.com/geolocation/v1/geolocate",
  "headers" : {
    "accept":"application/json",
    "Content-Type" : "application/json"
  },
  "query":{
    "key":"API_KEY"
  },
  "requestType": "POST",
  "body" : "{\"cellTowers\":[{{#towers}}{\"cellId\":{{id}},\"locationAreaCode\":{{lac}},\"mobileCountryCode\":{{mcc}},\"mobileNetworkCode\":{{mnc}} }{{/towers}}]}",
  "noDefaults":"true"
}
